    "@supabase/supabase-js": "^2.43.0",
    "dotenv": "^16.4.5",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "zod": "^3.23.8",
    "compression": "^1.7.4",
    "helmet": "^8.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.7",
    "@types/compression": "^1.7.5",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  }
}
