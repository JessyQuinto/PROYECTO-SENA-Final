{
  "name": "marketplace-backend-demo",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.optimized.js",
    "build": "tsc -p tsconfig.json",
    "build:optimized": "tsc src/index.optimized.ts --outDir dist --target ES2020 --module NodeNext --moduleResolution NodeNext --esModuleInterop --skipLibCheck"
  },
  "dependencies": {
